{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "name": {
      "type": "string"
    },
    "level": {
      "type": "enumeration",
      "enum": [
        "EASY",
        "MEDIUM",
        "HARD"
      ]
    },
    "learningDurationMin": {
      "type": "integer"
    },
    "description": {
      "type": "string"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::fa.fa",
      "inversedBy": "courses"
    },
    "serviceCategories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::service-category.service-category",
      "inversedBy": "courses"
    },
    "chapters": {
      "type": "dynamiczone",
      "components": [
        "shared.course-chapter"
      ]
    }
  }
}
