{
  "kind": "collectionType",
  "collectionName": "consultations",
  "info": {
    "singularName": "consultation",
    "pluralName": "consultations",
    "displayName": "Consultation"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fa": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::fa.fa",
      "inversedBy": "consultations"
    },
    "startTime": {
      "type": "datetime"
    },
    "endTime": {
      "type": "datetime"
    },
    "duration": {
      "type": "integer"
    },
    "summary": {
      "type": "blocks"
    },
    "consultationStatus": {
      "type": "enumeration",
      "enum": [
        "SCHEDULED",
        "CONSULTED",
        "REVIEWED"
      ]
    },
    "meetingLink": {
      "type": "string"
    },
    "meetingProvider": {
      "type": "enumeration",
      "enum": [
        "GOOGLE_MEET",
        "ZOOM",
        "TEAMS"
      ]
    },
    "isSentSummaryEmail": {
      "type": "boolean"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "consultations"
    }
  }
}
