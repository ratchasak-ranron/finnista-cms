{
  "kind": "collectionType",
  "collectionName": "consultations",
  "info": {
    "singularName": "consultation",
    "pluralName": "consultations",
    "displayName": "Consultation"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fa": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::fa.fa",
      "inversedBy": "consultations"
    },
    "start_time": {
      "type": "datetime"
    },
    "end_time": {
      "type": "datetime"
    },
    "duration": {
      "type": "integer"
    },
    "summary": {
      "type": "blocks"
    },
    "consultation_status": {
      "type": "enumeration",
      "enum": [
        "scheduled",
        "consulted",
        "summarized"
      ]
    },
    "meeting_link": {
      "type": "string"
    },
    "meeting_provider": {
      "type": "enumeration",
      "enum": [
        "google_meet",
        "zoom",
        "teams"
      ]
    }
  }
}
